                                .col-md-6
                                  img alt="" src="images/portfolio-2.jpg" /
                            .modal-footer
                              button.btn.btn-sm.btn-default data-dismiss="modal" type="button"  Close
                      /! Modal end
          /! isotope filters end
          /! section end
          /! section start
          /! ================
          .default-bg.space
            .container
              .row
                .col-md-8.col-md-offset-2
                  h1.text-center 1000+ Happy Drinkers
          /! section end
          /! .subfooter start
          /! ================
          .subfooter
            .container
              .row
                .col-md-12
                  p.text-center
                    a href="http://whiskyclub.herokuapp.com" target="_blank"  Whisky Club
