doctype html
html
  head
    title KM Whisky Club
    | <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %
    | <%= csrf_meta_tags %
    /! Mobile Meta
    meta content="width=device-width, initial-scale=1.0" name="viewport" /
    /! Favicon
    link href="images/favicon.ico" rel=("shortcut icon") /
    /! Web Fonts
    link href="https://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800" rel="stylesheet" type="text/css" /
    link href="https://fonts.googleapis.com/css?family=Raleway:700,400,300" rel="stylesheet" type="text/css" /
    /! Bootstrap core CSS
    link href="bootstrap/css/bootstrap.css" rel="stylesheet" /
    /! Font Awesome CSS
    link href="fonts/font-awesome/css/font-awesome.css" rel="stylesheet" /
    /! Plugins
    link href="css/animations.css" rel="stylesheet" /
    /! Worthy core CSS file
    link href="css/style.css" rel="stylesheet" /
    /! Custom css
    link href="css/custom.css" rel="stylesheet" /
    /! Fonts
    link href="https://fonts.googleapis.com/css?family=Josefin+Slab:100,300,400,600,700,100italic,300italic,400italic,600italic,700italic" rel="stylesheet" type="text/css" /
  body.no-trans
    .scrollToTop
      i.icon-up-open-big
    /! header start
    /! ================
    header.header.fixed.clearfix.navbar.navbar-fixed-top
      .container
        .row
          .col-md-4
            /! header-left start
            /! ================
            .header-left.clearfix
              /! logo
              .logo.smooth-scroll
                a href="#banner"
                  img#logo alt=("Whisky Club") src="images/logo.png" /
              /! name-and-slogan
              .site-name-and-slogan.smooth-scroll
                .site-name
                  a href="#banner"  Whisky Club Page
                .site-slogan
                  a href="http://whiskyclub.herokuapp.com" target="_blank"  Whisky Club
            /! header-left end
          .col-md-8
            /! header-right start
            /! ================
            .header-right.clearfix
              /! main-navigation start
              /! ================
              .main-navigation.animated
                /! navbar start
                /! ================
                nav.navbar.navbar-default role="navigation"
                  .container-fluid
                    /! Toggle get grouped for better mobile display
                    .navbar-header
                      button.navbar-toggle data-target="#navbar-collapse-1" data-toggle="collapse" type="button"
                        span.sr-only Toggle navigation
                        span.icon-bar
                        span.icon-bar
                        span.icon-bar
                    /! Collect the nav links, forms, and other content for toggling
                    #navbar-collapse-1.collapse.navbar-collapse.scrollspy.smooth-scroll
                      ul.nav.navbar-nav.navbar-right
                        li.active
                          a href="#banner"  Home
                        li
                          a href="#calendar"  Calendar
                        li
                          a href="#clients"  Clients
                        li
                          a href="#contact"  Contact
                        
                /! navbar end
              /! main-navigation end
            /! header-right end
    /! banner start
    /! ================
    #banner.banner
      .banner-image
      .banner-caption
        .container
          .row
            .col-md-8.col-md-offset-2.object-non-visible data-animation-effect="fadeIn"
              h1.text-center
                | Whisky
                span Club
              p.lead.text-center The heros of our whisky club
    /! banner end
    /! section start
    /! ================
    .default-bg.space.blue
      .container
        .row
          .col-md-8.col-md-offset-2
            h1.text-center Sponsor Us With a Whisky
    /! section end
    /! section start
    /! ================
    .section
      .container
        h1#calendar.text-center.title Whisky Sponsorship
        .separator
        p.lead.text-center Whisky sponsorhsip dates
        br/
        .row.object-non-visible data-animation-effect="fadeIn"
          .col-md-12
            /! isotope filters start
            .filters.text-center
              ul.nav.nav-pills
                li.active
                  a data-filter="*" href="#"  All
                li
                  a data-filter=".open-dates" href="#"  Open Dates
                li
                  a data-filter=".sponsor-dates" href="#"  Sponsored Dates
                li
                  a data-filter=".past-dates" href="#"  Past Dates
            /! isotope filters end
            /! portfolio items start
            .isotope-container.row.grid-space-20
              | <% Event.all.each do |e1| %
              | <% if e1.title == "Open" %
              .col-sm-6.col-md-3.isotope-item.open-dates
                | <% elsif e1.start_time > Date.today && e1.title != "Open" %
                .col-sm-6.col-md-3.isotope-item.sponsor-dates
                  | <% elsif e1.start_time < Date.today && e1.title != "Open" %
                  .col-sm-6.col-md-3.isotope-item.past-dates
                    | <% else %
                    .col-sm-6.col-md-3.isotope-item.past-dates
                      | <% end %
                      .image-box
                        .overlay-container
                          img alt="" src="images/portfolio-1.jpg" /
                          a.overlay data-target="#project-1" data-toggle="modal"
                            i.fa.fa-search-plus
                            span Sponsorship Details
                        a.btn.btn-default.btn-block data-target="#project-1" data-toggle="modal"
                          | <%= e1.title %
                          br>/
                          | <%= e1.start_time %
                      /! Modal
                      #project-1.modal.fade aria-hidden="true" aria-labelledby="project-1-label" role="dialog" tabindex="-1"
                        .modal-dialog.modal-lg
                          .modal-content
                            .modal-header
                              button.close data-dismiss="modal" type="button"
                                span aria-hidden="true"  &times;
                                span.sr-only Close
                              h4#project-1-label.modal-title <% e1.title %
                            .modal-body
                              h3 Project Description
                              .row
                                .col-md-6
