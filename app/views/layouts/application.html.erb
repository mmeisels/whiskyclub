                      /! Modal end
                    | <% end %
                    .col-sm-6.col-md-3.isotope-item.app-development
                      .image-box
                        .overlay-container
                          img alt="" src="images/portfolio-2.jpg" /
                          a.overlay data-target="#project-2" data-toggle="modal"
                            i.fa.fa-search-plus
                            span App Development
                        a.btn.btn-default.btn-block data-target="#project-2" data-toggle="modal"  Project Title
                      /! Modal
                      #project-2.modal.fade aria-hidden="true" aria-labelledby="project-2-label" role="dialog" tabindex="-1"
                        .modal-dialog.modal-lg
                          .modal-content
                            .modal-header
                              button.close data-dismiss="modal" type="button"
                                span aria-hidden="true"  &times;
                                span.sr-only Close
                              h4#project-2-label.modal-title Project Title
                            .modal-body
                              h3 Project Description
                              .row
                                .col-md-6
